.col_8
	width 620px
.col_9
	width 700px
.col_10
	width 780px
.col_11
	width 860px
.col_12
	position relative
	width 940px
.text-center
	text-align center
.text-right
	text-align right
.text-left
	text-align left
.btn
	border-radius 9px
	border none
	border-bottom 6px solid #256ea2
	box-shadow none
	text-transform uppercase
	color #ffffff
	background #2f8dd0
	background url('data:image/jpeg;base64,')
	.hover
		display none
	&:hover
		background #349ee9
		background url('data:image/jpeg;base64,')
		.hover
			display inline
		.normal
			display none
.input
	border-radius 9px
	border 3px solid rgb( 255, 213, 1 )
	background #fff
	&.default
		background #fff
		border 1px solid rgb( 55, 131, 155 )
.large
	width 340px
	box-sizing border-box
	padding 30px 20px
	font-size 31px
	text-align center
	&.btn
		padding-bottom 27px
.mid
	width 250px
	box-sizing border-box
	padding 20px 10px
	font-size 24px
	text-align center
	&.btn
		padding-bottom 17px
		border-bottom-width 3px
.foot-text
	font-size 13px
	color rgb( 135, 135, 135 )
	margin-bottom 60px
	a
		color rgb( 135, 135, 135 )
		text-decoration underline
		&:hover
			text-decoration none
	ul
		list-style none
		margin 12px 0
		li
			display inline
.container
	.marginb20
		margin-bottom 20px
	.margint40
		margin-top 40px
	.margint20
		margin-top 20px
	.margintb60
