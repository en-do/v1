@keyframes a_arrow_l {
	from {
		left 270px
	}
	50% {
		left 245px
	}
	to {
		left 270px
	}
}
@keyframes a_arrow_r {
	from {
		right 270px
	}
	50% {
		right 245px
	}
	to {
		right 270px
	}
}
.arrow_l
	background arrow_l center center no-repeat
	position absolute
	left 270px
	top 39px
	width 24px
	height 24px
	display block
	animation a_arrow_l 2s infinite
.arrow_r
	background arrow_r center center no-repeat
	position absolute
	right 270px
	top 39px
	width 24px
	height 24px
	display block
	animation a_arrow_r 2s infinite



