/* Flows */
.dan_flow,
.janny_flow
	.main_title
		font-size 40px
		letter-spacing -1.1px
.bg_wh
	background #ffffff
.form_title
	font-family 'Lato', sans-serif
	font-size 25px
	color blue
	text-align center
	line-height 2.5em
/* End of Flows */

/* Car Steps */
.car_steps
	position absolute
	top 70px
	right 215px
	width 30px
	height 181px
	.step
		display inline-block
.car_cnt
	position absolute
	top 27px
	left 100%
	width 223px
	height 181px
.car_color,
.car_black
	position absolute
	top 0
	left 0
	width 223px
	height 181px
	background sprite -223px 0 no-repeat
.car_black
	background sprite 0 0 no-repeat	
.st_title
	display inline-block
	font-size 11px
	color inactive
	padding 0 3px
.round
	display inline-block
	box-sizing border-box
	width 30px
	height 30px
	font-size 16px
	color #ffffff
	text-align center
	padding 3px 0 0 0
	background inactive
	border-radius 50%
.step1
	.car_black
		height 115px
.step2
	.car_black
		height 67px
.step3
	.car_black
		height 0px
.step_active
	.st_title
		color blue
	.round
		background blue
/* End of Car Steps */

/* inputs */
.form
	color #000
.input_container
	position relative
	padding 3px 0 12px 0
	margin 0 auto
	width 300px
.label 
	position absolute
	right 100%
	top 0
	width 350px
	text-align right
	span
		font-family 'Raleway', sans-serif
		font-size 20px
		font-weight normal
		color blue
		padding 0 1em 0 0
		line-height 2.5em

.field
	box-sizing border-box

.field_input,
.field_select
	font-family 'Raleway', sans-serif
	font-size 20px
	font-weight normal
	width 100%
	border 1px solid blue
	border-radius 5px
.field_input,
.field_select
	padding 8.6px 7px
	border 1px solid blue

.i_icon
	display none
	position absolute
	top 14px
	right -34px
	width 23px
	height 23px
.i_cal
	position absolute
	top 14px
	right 14px
	width 16px
	height 16px
	background sprite -527px -46px no-repeat
.field_input:focus:required:invalid + .i_icon,
.field_select:focus:required:invalid + .i_icon
	display block
	background sprite -448px -100px
.field_input:required:valid + .i_icon,
.field_select:required:valid + .i_icon
	display block
	background sprite -472px -100px
.radio_cnt
	padding 8.6px 7px
	text-align center
.radio
	display inline-block
	position relative
	font-family 'Raleway', sans-serif
	font-size 20px
	font-weight normal
	padding 0 16px 0 20px
	cursor pointer
	.i_rad,
	.i_che
		position absolute
		top 3px
		left -8px
		width 19px
		height 19px
		background sprite -447px -65px 
	.i_che
		background sprite -447px -45px
	input
		position absolute
		top 6px
		left -4px
	input:checked + .i_rad
		background sprite -487px -65px
	input:checked + .i_che
		background sprite -487px -45px
/* End of inputs */
.radio
	&:hover
		.radio_i
			background sprite -20px -99px
		input:checked + .radio_i
			background sprite -60px -99px

input[type='number']
	-moz-appearance textfield
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button
	-webkit-appearance none

/* Exit pop */
arrow_l3 = url('data:image/png;base64,')
arrow_r3 = url('data:image/png;base64,')

#overlay
	position fixed
	display none
	top 0
	left 0
	width 100%
	height 100%
	background overback
	z-index 99
	overflw hidden
#exit_pop
	display none
	position fixed
	z-index 102
	.arrow_l2
		background arrow_l3 center center no-repeat
		position absolute
		left 90px
		top 247px
		width 24px
		height 24px
		display block
	.arrow_r2
		background arrow_r3 center center no-repeat
		position absolute
		right 90px
		top 247px
		width 24px
		height 24px
		display block
.popup
	position absolute
	z-index 100
.close
	position absolute
	top 10px 
	right 10px
	width 11px
	height 11px
	background close no-repeat
	cursor pointer
.dan_exit
	position absolute
	top 21px 
	left -149px

.popup_title,
.popup_subtitle
	margin 0
	color #ffa532
	z-index 100
	text-align center
		
.pop_big
	top 0
	left 50%
	width 660px
	margin 50px 0 0 -330px
	background #fff
	.popup_title
		font-size 53px
		font-weight 600
		line-height 1em
		color #ff4208
		margin 50px 0 8px
	.popup_subtitle
		font-size 40px
		font-weight 400
		line-height 1.3em
		margin 0 0 44px 0 
		color #37839b
		letter-spacing -1px
	.btn
		font-size 40px
		background-size 100% 100%
		width 420px
		margin-bottom 78px
		padding-bottom 23px
		padding-top 23px
		&:hover
			background-size 100% 100%
			


.steps_errors
	display none
	padding 20px 0 0 0
	span
		color #000000
		padding 10px
		background #ffe17a
	


.yellow_man
	position absolute
	bottom -1px
	right 100px
	width 169px
	height 170px
	background yellow_man no-repeat
